{
	"containerDefinitions": [{
		"name": "bot",
		"image": "614459117250.dkr.ecr.eu-west-1.amazonaws.com/eyewitness-app:latest",
		"memoryReservation": 96,
		"essential": true,
		"portMappings": [{
			"containerPort": 5000,
			"hostPort": 0,
			"protocol": "tcp"
		}],
		"environment": [{
			"name": "NODE_ENV",
			"value": "{{REPLACE_ME}}"
		}, {
			"name": "PROVIDER_ID",
			"value": "battabox"
		}, {
			"name": "ENTRY_POINT",
			"value": "bot"
		}],
		"logConfiguration": {
			"logDriver": "awslogs",
			"options": {
				"awslogs-region": "{{REPLACE_ME}}",
				"awslogs-group": "{{REPLACE_ME}}",
				"awslogs-stream-prefix": "bot"
			}
		}
	}, {
		"name": "read-server",
		"image": "614459117250.dkr.ecr.eu-west-1.amazonaws.com/eyewitness-app:latest",
		"memoryReservation": 96,
		"essential": true,
		"portMappings": [{
			"containerPort": 5001,
			"hostPort": 0,
			"protocol": "tcp"
		}],
		"environment": [{
			"name": "NODE_ENV",
			"value": "{{REPLACE_ME}}"
		}, {
			"name": "PROVIDER_ID",
			"value": "battabox"
		}, {
			"name": "ENTRY_POINT",
			"value": "read-server"
		}],
		"logConfiguration": {
			"logDriver": "awslogs",
			"options": {
				"awslogs-region": "{{REPLACE_ME}}",
				"awslogs-group": "{{REPLACE_ME}}",
				"awslogs-stream-prefix": "read-server"
			}
		}
	}]
}
