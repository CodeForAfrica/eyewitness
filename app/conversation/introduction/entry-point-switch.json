{
	"uri": "/introduction/entry-point-switch",
	"type": "basic",
	"actions": [{
		"type": "update-memory",
		"memory": {
			"lastChosenActivity": {
				"operation": "unset"
			}
		}
	}, {
		"type": "change-step",
		"conditional": "!<referral> || <referral> === <lastReferral>",
		"nextUri": "/activities"
	}, {
		"type": "update-memory",
		"memory": {
			"lastReferral": {
				"reference": "<referral>"
			}
		}
	}, {
		"type": "change-step",
		"nextUri": "/activities/latest-stories"
	}]
}
